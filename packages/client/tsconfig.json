{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "target": "esnext",
        "lib": [
            "dom",
            "esnext"
        ],
        "baseUrl": ".",
        "outDir": "./lib",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "paths": {
            "mattermost-redux/types/*": ["../mattermost-redux/src/types/*"]
        },
        "rootDirs": [
            "./src",
        ]
    },
    "include": [
        "./src/**/*",
        "../mattermost-redux/src/types/*"
    ],
    "exclude": [
        "!node_modules/@types"
    ],
}
